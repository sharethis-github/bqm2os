select url, kw from flatten((
SELECT
    SPLIT(REGEXP_REPLACE(keywords.text, r'(_|-)+', ' '), " ") kw, url
  FROM
    FLATTEN((
      SELECT
        url,
        keywords.text,
        estidAccepted
      FROM
        {dataset}.mergelog_mini_sample
      WHERE
        keywords.text IS NOT NULL
        AND estidAccepted
        AND FLOAT(keywords.score) > 0.5), keywords)
), kw)
group each by url, kw
